{
  "displayName": "VogAPI",
  "name": "vogapi",
  "version": "1.0.0",
  "description": "Just a simple twitch api helper",
  "license": "MIT",
  "author": {
    "name": "Voguh",
    "email": "voguhofc@protonmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voguh/vogapi.git"
  },
  "scripts": {
    "start": "node ./dist/Main.js",
    "dev": "nodemon --exec 'ts-node' -r dotenv/config -r tsconfig-paths/register ./src/Main.ts",
    "build": "babel ./src --out-dir=\"./dist\" --config-file=\"./babel.config.js\" --extensions=\".ts,.js\" --copy-files --no-copy-ignored --no-comments",
    "lint": "eslint ./src --ext ts,tsx"
  },
  "dependencies": {
    "@twurple/api": "^7.1.0",
    "@twurple/auth": "^7.1.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
