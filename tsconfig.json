{
  "include": ["__tests__", "src"],
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "lib": ["ES6", "ESNext"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "sourceMap": false,
    "removeComments": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "vogapi/*": ["./src/*"]
    }
  },
  "ts-node": {
    "files": true,
    "transpileOnly": true
  }
}